    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "programa_driver.e",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/programa_driver.e",
            "args": ["<", "jp_public_token.in", ">", "sortida_token.res"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            
        }
],
}